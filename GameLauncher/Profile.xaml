<Window x:Class="GameLauncher.Profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameLauncher"
        mc:Ignorable="d"
        Title="Profile" Height="450" Width="300">
    <Grid>
        <Label x:Name="lblName" Content="Label" HorizontalAlignment="Left" Margin="27,28,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblUsername" Content="Label" HorizontalAlignment="Left" Margin="27,67,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblEmail" Content="Label" HorizontalAlignment="Left" Margin="27,113,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblRegisterDate" Content="Label" HorizontalAlignment="Left" Margin="27,169,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
